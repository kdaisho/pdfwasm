		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.53.0",
		"@tailwindcss/vite": "^4.2.1",
		"eslint": "^9.39.3",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.15.0",
		"eslint-plugin-tailwindcss": "4.0.0-beta.0",
		"eslint-plugin-tree-shaking": "^1.12.2",
		"globals": "^17.3.0",
		"husky": "^9.1.7",
