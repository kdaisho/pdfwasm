		"svelte": "^5",
		"svelte-check": "^4",
		"svelte-eslint-parser": "^1.5.1",
		"tailwindcss": "^4.2.1",
		"typescript": "~5.6.2",
		"typescript-eslint": "^8.56.1",
		"vite": "^6.0.5",
